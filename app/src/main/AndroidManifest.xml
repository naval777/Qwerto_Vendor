        <activity android:name=".Activities.MyProperties"/>
        <activity android:name=".Activities.PropertyType"/>
        <activity android:name=".Activities.PersonalInfo"/>


        <activity android:name=".SetText"
            android:windowSoftInputMode="stateVisible">

        </activity>
        <activity android:name=".Activities.SetImages">

        </activity>

    </application>

</manifest>
